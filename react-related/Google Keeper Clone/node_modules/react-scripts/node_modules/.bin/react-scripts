#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Portfolio-Projects/react-related/Google Keeper Clone/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.1_@babel+core@7.24.4__@babel+plugin-transf_5kdtiuidapjyomcgiusnbvfjwe/node_modules/react-scripts/bin/node_modules:/mnt/d/Portfolio-Projects/react-related/Google Keeper Clone/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.1_@babel+core@7.24.4__@babel+plugin-transf_5kdtiuidapjyomcgiusnbvfjwe/node_modules/react-scripts/node_modules:/mnt/d/Portfolio-Projects/react-related/Google Keeper Clone/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.1_@babel+core@7.24.4__@babel+plugin-transf_5kdtiuidapjyomcgiusnbvfjwe/node_modules:/mnt/d/Portfolio-Projects/react-related/Google Keeper Clone/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Portfolio-Projects/react-related/Google Keeper Clone/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.1_@babel+core@7.24.4__@babel+plugin-transf_5kdtiuidapjyomcgiusnbvfjwe/node_modules/react-scripts/bin/node_modules:/mnt/d/Portfolio-Projects/react-related/Google Keeper Clone/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.1_@babel+core@7.24.4__@babel+plugin-transf_5kdtiuidapjyomcgiusnbvfjwe/node_modules/react-scripts/node_modules:/mnt/d/Portfolio-Projects/react-related/Google Keeper Clone/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.1_@babel+core@7.24.4__@babel+plugin-transf_5kdtiuidapjyomcgiusnbvfjwe/node_modules:/mnt/d/Portfolio-Projects/react-related/Google Keeper Clone/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../../bin/react-scripts.js" "$@"
fi
